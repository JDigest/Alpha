<h1>Listing tag_topics</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th>Topic</th>
      <th>Weight</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tag_topics.each do |tag_topic| %>
      <tr>
        <td><%= tag_topic.tag %></td>
        <td><%= tag_topic.topic %></td>
        <td><%= tag_topic.weight %></td>
        <td><%= link_to 'Show', tag_topic %></td>
        <td><%= link_to 'Edit', edit_tag_topic_path(tag_topic) %></td>
        <td><%= link_to 'Destroy', tag_topic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tag topic', new_tag_topic_path %>
